#!/bin/sh

# Basic checks about case-sensibility of ratfor

. ../lib/rat4-t.sh || exit 99


echo fooBAR > tst.r

run_RATFOR tst.r || testcase_FATAL_FAILURE
test -s stderr && testcase_FATAL_FAILURE

$FGREP 'FOO' stdout && testcase_FATAL_FAILURE
$FGREP 'bar' stdout && testcase_FATAL_FAILURE
$FGREP 'fooBAR' stdout || testcase_FATAL_FAILURE

testcase_PASS

