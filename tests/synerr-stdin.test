#!/bin/sh

# Check that the name of standard input in error messages is "(stdin)",
# not "-".

. ../lib/rat4-testsuite-init.sh || exit 99

echo '}' > tst.r
run_RATFOR -e 1 <tst.r || testcase_FAIL
$GREP '^-' stderr && testcase_FAIL
$GREP '^(stdin):' stderr || testcase_FAIL

testcase_DONE

