#!/bin/sh

# Check that the name of standard input in error messages is "(stdin)",
# not "-".

. ../lib/rat4-t.sh || exit 99

echo '}' > tst.r
run_RATFOR -e 1 <tst.r || testcase_FATAL_FAILURE
$GREP '^-' stderr && testcase_FATAL_FAILURE
$GREP '^(stdin):' stderr || testcase_FATAL_FAILURE

testcase_DONE

