#!/bin/sh

# Check that ratfor does not overwrite its output file if the opening of
# the input file fails.

. ../lib/rat4-t.sh || exit 99

mkdir tst.out
echo 'saved' > safe
cp safe risky

test ! -f none.r

run_RATFOR -e 1 -- -o risky none.r || testcase_FAIL
cat risky

set +x
(set +x; $DIFF_U safe risky) || { echo '~~~~~'; testcase_FAIL; }
set -x

testcase_DONE

