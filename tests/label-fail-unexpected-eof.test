#!/bin/sh

# Check that ratfor complains if a label is followed by EOF

. ../lib/rat4-t.sh || exit 99

cat >tst.r <<EOF
foo
bar
100
EOF

cat tst.r

run_RATFOR -e 1 tst.r || testcase_FATAL_FAILURE
$FGREP ':3: unexpected EOF.' stderr || testcase_FATAL_FAILURE

testcase_DONE

