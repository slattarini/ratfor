#!/bin/sh

# Check that `-o -' tells ratfor to write on standard output

. ../lib/rat4-t.sh || exit 99

echo 'junk123' > tst.r

run_RATFOR -o - tst.r || testcase_FATAL_FAILURE
test -f ./-  && testcase_FATAL_FAILURE
test -s stderr && testcase_FATAL_FAILURE
$GREP 'junk123' stdout || testcase_FATAL_FAILURE

testcase_PASS

