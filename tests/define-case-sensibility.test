#!/bin/sh

# Check that 'define' is case-sensible w.r.t. the symbol name.

. ./defs.sh || exit 99

cat >tst.r <<EOF
define sym1 foo
define sym2 bar
define SYM2 quux
sym1
SYM1
sym2
SYM2
EOF

cat >exp <<EOF
foo
sym1
bar
quux
EOF

run_RATFOR -e 0 tst.r || testcase_FAIL
$GREP -v '^[cC]' <stdout | ws_normalize >got

$DIFF_U exp got || testcase_FAIL

testcase_PASS

# vim: ft=sh ts=4 sw=4 et
