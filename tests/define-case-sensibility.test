#!/bin/sh

# Check that 'define' is case-sensible w.r.t. the symbol name.

. ../lib/rat4-t.sh || exit 99

cat >tst.r <<EOF
define sym1 foo
define sym2 BAR
define SYM2 quux
sym1
SYM1
sym2
SYM2
EOF

cat >exp <<EOF
foo
SYM1
BAR
quux
EOF

cat tst.r

run_RATFOR tst.r || testcase_FATAL_FAILURE
$GREP -v '^[cC]' <stdout | ws_normalize >got

cat exp
cat got

$DIFF_U exp got || testcase_FATAL_FAILURE

testcase_DONE

# vim: ft=sh ts=4 sw=4 et
