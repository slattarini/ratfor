#!/bin/sh

# Check that ratfor does not give spurious errors about
# "unbalanced parentheses".

. ./TestsAux/rat4-t.sh || exit 99

cat >tst.r <<EOF
x=(y)
(a)
()
((b))
(())
(a(b(c(d)e)f)g)h
a(b(c(d(e)f)g)h)
EOF

run_RATFOR tst.r || testcase_FAIL
test -s stderr && testcase_FAIL

testcase_PASS

