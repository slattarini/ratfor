#!/bin/sh

# Test that `%' to output lines verbatim is recognized only if placed
# at the beginning of a line

. ../lib/rat4-t.sh || exit 99

cat >tst.r <<EOF
a = 1 %&&
b = 1; %&&
 %&&
EOF

run_RATFOR tst.r || testcase_FATAL_FAILURE
$FGREP "&&" stdout && testcase_FATAL_FAILURE

testcase_DONE

