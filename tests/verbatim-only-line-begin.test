#!/bin/sh

# Test that `%' to output lines verbatim is recognized only if placed
# at the beginning of a line

. ../lib/rat4-testsuite-init.sh || exit 99

cat >tst.r <<EOF
a = 1 %&&
b = 1; %&&
 %&&
EOF

cat tst.r

run_RATFOR tst.r || testcase_FAIL
$FGREP "&&" stdout && testcase_FAIL

testcase_DONE

