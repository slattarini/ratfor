#!/bin/sh

# Check that ratfor complains if te keyword `fuction' is not followed by
# a fuction name.

. ./TestsAux/defs.sh || exit 99

echo function > tst1.r
run_RATFOR -e 1 tst1.r || testcase_FAIL
mv stderr stderr1
$FGREP 'missing function name' stderr1 || testcase_FAIL

echo 'function; foo' > tst2.r
run_RATFOR -e 1 tst2.r || testcase_FAIL
mv stderr stderr2
$FGREP 'missing function name' stderr2 || testcase_FAIL

testcase_PASS

