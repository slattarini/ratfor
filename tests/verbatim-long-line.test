#!/bin/sh

# Check that `%' to output verbatim also very long lines

. ./TestsAux/defs.sh || exit 99

s=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
longline=$s$s$s$s$s$s$s$s$s$s$s$s$s

echo "%$longline" >tst.r

run_command "$RATFOR" tst.r || testcase_FAIL
$GREP "^$longline$" stdout || testcase_FAIL

testcase_PASS

