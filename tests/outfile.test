#!/bin/sh

# Basic checks on the `-o' option of ratfor.

. ../lib/rat4-t.sh || exit 99

echo 'junk123' > tst.r

run_RATFOR -o tst.f tst.r || testcase_FATAL_FAILURE
test -f tst.f || testcase_FATAL_FAILURE
cat tst.f
test -s stdout && testcase_FATAL_FAILURE
test -s stderr && testcase_FATAL_FAILURE
$GREP 'junk123' tst.f || testcase_FATAL_FAILURE

testcase_PASS

