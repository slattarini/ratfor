#!/bin/sh

# Check the `INCLUDE' is not an alias for `include'.

. ./TestsAux/defs.sh || exit 99

echo 'FAILURE' >notincluded.r
echo 'INCLUDE notincluded.r' >tst.r

run_CMD "$RATFOR" tst.r || testcase_FAIL
test -s stderr && testcase_FAIL
$FGREP INCLUDE stdout || testcase_FAIL
$FGREP -i failure stdout && testcase_FAIL

testcase_PASS

