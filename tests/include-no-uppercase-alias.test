#!/bin/sh

# Check the `INCLUDE' is not an alias for `include'.

. ../lib/rat4-t.sh || exit 99

echo 'FAILURE' >notincluded.r
echo 'INCLUDE notincluded.r' >tst.r

run_RATFOR tst.r || testcase_FATAL_FAILURE
test -s stderr && testcase_FATAL_FAILURE
$FGREP INCLUDE stdout || testcase_FATAL_FAILURE
$FGREP -i failure stdout && testcase_FATAL_FAILURE

testcase_PASS

