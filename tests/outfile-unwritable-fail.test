#!/bin/sh

# Check that ratfor barfs if given an unwritable file output file.

. ../lib/rat4-t.sh || exit 99

: > tst.out
chmod a-w tst.out
echo 'junk' > tst.r

run_RATFOR -e 1 -- -o tst.out tst.r || testcase_FAIL
rx='tst\.out: cannot open for writing: [pP]ermission denied'
$GREP  "$rx" stderr || testcase_FAIL

testcase_DONE

