#!/bin/sh

# Check that ratfor diagnose possible conflicts between internally-used
# labels and used-provided labels.

. ./TestsAux/defs.sh || exit 99

cat >tst.r <<EOF
while(true) {
    do_nothing()
}
100 continue
EOF

run_CMD -e 1 "$RATFOR" -l 100 tst.r || testcase_FAIL
$FGREP ':4: possible label conflict' stderr || testcase_FAIL

testcase_PASS

