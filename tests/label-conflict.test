#!/bin/sh

# Check that ratfor diagnose possible conflicts between internally-used
# labels and used-provided labels.

. ../lib/rat4-t.sh || exit 99

cat >tst.r <<EOF
while(true) {
    do_nothing()
}
100 continue
EOF

run_RATFOR -e 1 -- -l 100 tst.r || testcase_FATAL_FAILURE
$FGREP ':4: possible label conflict' stderr || testcase_FATAL_FAILURE

testcase_PASS

