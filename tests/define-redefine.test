#!/bin/sh

# Check tha symbols defined with `define' can be redefined.

. ./TestsAux/defs.sh || exit 99

cat >tst.r <<EOF
define sym foo
sym
define sym bar
sym
define foo quux
sym
foo
EOF

cat >exp <<EOF
foo
bar
bar
quux
EOF

run_RATFOR tst.r || testcase_FAIL
$GREP -v '^[cC]' stdout | ws_normalize > got

cat exp
cat got

$DIFF_U exp got || testcase_FAIL

testcase_PASS

