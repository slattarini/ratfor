#!/bin/sh

# Check that `-C' switch of ratfor preserve comments in output.

. ../lib/rat4-t.sh || exit 99

cat >tst.r <<EOF
# this is a "%c@mm3nt%'!
blah blah blah
EOF

run_RATFOR -C tst.r || testcase_FATAL_FAILURE
$GREP "^[cC].*\"%c@mm3nt%'!" stdout || testcase_FATAL_FAILURE
testcase_PASS

