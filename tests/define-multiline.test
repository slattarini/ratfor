#!/bin/sh

# Check that `define(SYM, VALUE)' works on multi-line VALUE.

. ../lib/rat4-t.sh || exit 99

cat >tst.r <<EOF
define(sym,
line1
line2
line3)
sym
alive
EOF

cat > exp <<EOF
line1
line2
line3
alive
EOF

cat tst.r

run_RATFOR tst.r || testcase_FATAL_FAILURE
$GREP -v '^[cC]' stdout | ws_normalize >got

cat exp
cat got

$DIFF_U exp got || testcase_FATAL_FAILURE

testcase_PASS

