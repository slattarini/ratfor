#!/bin/sh

# Check that 'define(SYMBOL,...)' preserve leading/trailing spaces

. ./TestsAux/defs.sh || exit 99

cat >tst.r <<EOF
define(sym1, text1)
define(sym2,text2 )
define(sym3, text3 )
a:sym1:a
b:sym2:b
c:sym3:c
EOF

run_CMD "$RATFOR" tst.r || testcase_FAIL

$FGREP 'a: text1:a' stdout  || testcase_FAIL
$FGREP 'b:text2 :b' stdout  || testcase_FAIL
$FGREP 'c: text3 :c' stdout || testcase_FAIL

testcase_PASS

# vim: ft=sh ts=4 sw=4 et
