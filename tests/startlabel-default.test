#!/bin/sh

# Check that ratfor uses 23000 as default internal starting label

. ./TestsAux/defs.sh || exit 99

cat >tst.r <<EOF
while(true) {
    do_nothing()
}
EOF

run_command "$RATFOR" tst.r || testcase_FAIL
$GREP -v '^[cC]' stdout >stdout1
$GREP '^23000 ' stdout1 || testcase_FAIL
$EGREP -v '^( |2300[0-9]) ' && testcase_FAIL

testcase_PASS

