#!/bin/sh

# Check that ratfor uses 23000 as default internal starting label

. ../lib/rat4-t.sh || exit 99

cat >tst.r <<EOF
while(true) {
    do_nothing()
}
EOF

run_RATFOR tst.r || testcase_FATAL_FAILURE
$GREP -v '^[cC]' stdout >stdout1
$GREP '^23000 ' stdout1 || testcase_FATAL_FAILURE
$EGREP -v '^( |2300[0-9]) ' && testcase_FATAL_FAILURE

testcase_PASS

