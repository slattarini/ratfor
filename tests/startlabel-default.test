#!/bin/sh

# Check that ratfor uses 23000 as default internal starting label.

. ../lib/rat4-testsuite-init.sh || exit 99

cat >tst.r <<EOF
while(true) {
    do_nothing()
}
EOF

cat tst.r

run_RATFOR tst.r || testcase_FAIL
$GREP -v '^[cC]' stdout >stdout1
$GREP '^23000 ' stdout1 || testcase_FAIL
$EGREP -v '^( |2300[0-9]) ' stdout1 && testcase_FAIL

testcase_DONE

# vim: ft=sh ts=4 sw=4 et
